@import "../../../utils/style/theme.wxss";
.navigate{
  height: 200rpx;
}
.box{
  background: white;
  margin-top:20rpx;
  width: 100%;
  display: flex;
  flex-direction: column
}
.navigate text:first-child{
  height: 80rpx;
  line-height: 80rpx;
  margin-left: 20rpx;
}
.navigate text:last-child{
  height: 120rpx;
  padding-top: 20rpx;
  margin-left: 20rpx;
}
.task-line{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  border-bottom:  solid 0.1px #e1e1e1;
  position: relative;
}
.task-line text:first-child{
  margin-left: 20rpx;
}
.task-line text:last-child{
  position: absolute;
  right: 20rpx;
  color: black;
}
.task-line .mid{
  position: absolute;
  right: 50rpx;
  top:4rpx;
}